import numpy as np
a = [1, 2, 4]
b = np.array(a)
print(type(a))
print(type(b))
print(b * b)
print(b - b)
print(a / b)
A = np.zeros((2, 3))
print(A)
A[0, 2] = 5
print(A)
B = np.ones((3, 2))
print(B)
A = np.ndarray(shape=(2, 3))
print(A)
C = np.arange(1, 10, 1)
print(C)
print(C[-1])
print(len(C))
n = np.linspace(0, 10, 10)
print(n)
m=np.array([[1,2,3],[4,5,6]])
print(m[:, 1])
f=m[:, ::-1]
print(f)
